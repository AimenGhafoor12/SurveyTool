            @Html.DisplayFor(modelItem => item.feedbackString)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.rating)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Responder.responder1)
        </td>
